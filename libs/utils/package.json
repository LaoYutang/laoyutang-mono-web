{
  "name": "utils",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "require": "./dist/main.cjs",
      "import": "./dist/main.js"
    },
    "./hooks": {
      "types": "./dist/hooks.d.ts",
      "import": "./dist/hooks.js",
      "require": "./dist/hooks.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@types/node": "^18.14.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-plugin-dts": "2.0.0-beta.0"
  },
  "author": "ewen.zhang",
  "dependencies": {
    "axios": "^1.3.3"
  }
}
