{
  "name": "components",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "node ./script/build.js"
  },
  "main": "./build/components.js",
  "module": "./build/components.mjs",
  "types": "./build/index.d.ts",
  "style": "./build/style.css",
  "exports": {
    ".": {
      "require": "./build/components.js",
      "import": "./build/components.mjs",
      "types": "./build/index.d.ts"
    },
    "./build/*": "./build/*/index.mjs",
    "./build/*/style.css": "./build/*/style.css",
    "./build/style.css": "./build/style.css",
    "./*": "./*"
  },
  "peerDependencies": {
    "element-plus": ">=2.2.32",
    "utils": "workspace:*",
    "vue": ">=3.2.45"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "echarts": "^5.4.1",
    "element-plus": "^2.2.32",
    "utils": "workspace:*",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.26",
    "@types/node": "^18.14.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "fs-extra": "^11.1.0",
    "imports": "workspace:*",
    "sass": "^1.58.3",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.14.3",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^4.1.0",
    "vite-plugin-dts": "2.0.0-beta.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^1.0.24"
  }
}
